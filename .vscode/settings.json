{
	"files.exclude": {
		"**/.git": true,
		"**/.svn": true,
		"**/.hg": true,
		"**/CVS": true,
		"**/.DS_Store": true,
		"**/*.o": true,
		"**/*.exe": true,
	},
	"files.associations": {
		"*.js": "javascript",
		"iostream": "cpp",
		"cmath": "cpp",
		"new": "cpp",
		"iosfwd": "cpp",
		"*.tcc": "cpp",
		"fstream": "cpp",
		"ostream": "cpp"
	}
}